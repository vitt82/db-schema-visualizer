{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Prisma Extension (Dev)",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/prisma-vs-code-extension"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/prisma-vs-code-extension/dist/**/*.js"
      ],
      "preLaunchTask": "prisma: dev"
    },
    {
      "name": "Run DBML Extension (Dev)",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/dbml-vs-code-extension"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/dbml-vs-code-extension/dist/**/*.js"
      ],
      "preLaunchTask": "dbml: dev"
    }
  ]
}
